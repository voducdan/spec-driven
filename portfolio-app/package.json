{
  "name": "airflow-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A web-based portfolio project using Vite with minimal libraries, inspired by Apache Airflow DAG visualization.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "echo 'No TypeScript to check'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "portfolio",
    "airflow",
    "vite",
    "javascript"
  ],
  "author": "Dan Vo",
  "license": "MIT",
  "devDependencies": {
    "vite": "^5.4.0",
    "eslint": "^8.57.0",
    "prettier": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "vitest": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "jsdom": "^24.1.0"
  }
}
